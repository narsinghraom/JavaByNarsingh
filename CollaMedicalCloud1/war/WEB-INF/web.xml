<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	 
    <servlet>
        <servlet-name>AdminServlet</servlet-name>
        <servlet-class>colla.medical.controller.AdminServlet</servlet-class>
        
    </servlet>
    <servlet-mapping>
        <servlet-name>AdminServlet</servlet-name>
        <url-pattern>/AdminServlet</url-pattern>
    </servlet-mapping>
     <servlet>
        <servlet-name>EmpServlet</servlet-name>
        <servlet-class>colla.medical.controller.EmpServlet</servlet-class>
        
    </servlet>
    <servlet-mapping>
        <servlet-name>EmpServlet</servlet-name>
        <url-pattern>/EmpServlet</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>colla.medical.controller.LoginServlet</servlet-class>
        
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    
    
    <servlet>
        <servlet-name>PatientServlet</servlet-name>
        <servlet-class>colla.medical.controller.PatientServlet</servlet-class>
        
    </servlet>
    <servlet-mapping>
        <servlet-name>PatientServlet</servlet-name>
        <url-pattern>/PatientServlet</url-pattern>
    </servlet-mapping>
    
    
     <servlet>
        <servlet-name>PrescriptionServlet</servlet-name>
        <servlet-class>colla.medical.controller.PrescriptionServlet</servlet-class>
        
    </servlet>
    <servlet-mapping>
        <servlet-name>PrescriptionServlet</servlet-name>
        <url-pattern>/PrescriptionServlet</url-pattern>
    </servlet-mapping>
    
    
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>

</web-app>
